{
  "name": "discohook",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max-http-header-size=81920 next start",
    "build": "next build",
    "dev": "cross-env NODE_OPTIONS=--max-http-header-size=81920 next dev",
    "test": "cross-env NODE_ENV=test TZ=UTC jest",
    "test:watch": "pnpm run test -- --watch",
    "test:cov": "pnpm run test -- --coverage",
    "test:verbose": "pnpm run test:cov -- --verbose",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "pnpm run lint -- --fix",
    "lint:error": "pnpm run lint -- --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "pnpm run typecheck -- --watch",
    "format": "prettier --write './**/*.@(js|jsx|ts|tsx)'",
    "format:check": "prettier --check './**/*.@(js|jsx|ts|tsx)'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/discohook/discohook.git"
  },
  "author": "Discohook authors <hello@discohook.org>",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/discohook/discohook/issues"
  },
  "homepage": "https://discohook.org/",
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "@popperjs/core": "^2.4.4",
    "core-js": "^3.6.5",
    "d3-ease": "^1.0.6",
    "date-fns": "^2.14.0",
    "highlight.js": "^9.18.1",
    "idb": "^5.0.4",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.7",
    "next": "^9.4.4",
    "polished": "^3.6.5",
    "punycode": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-focus-on": "^3.4.1",
    "react-is": "^16.13.1",
    "react-spring": "^8.0.27",
    "regenerator-runtime": "^0.13.5",
    "simple-markdown": "^0.7.2",
    "styled-components": "^5.1.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@next/bundle-analyzer": "^9.4.4",
    "@testing-library/dom": "^7.20.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "@testing-library/user-event": "^12.0.11",
    "@types/d3-ease": "^1.0.9",
    "@types/highlight.js": "^9.12.4",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.22",
    "@types/react": "^16.9.42",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.1",
    "@types/testing-library__jest-dom": "^5.9.1",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-plugin-styled-components": "^1.10.7",
    "core-js-bundle": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "eslint-plugin-unicorn": "^20.1.0",
    "fake-indexeddb": "^3.1.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.6"
  }
}
